{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build (MinGW64)",
      "type": "shell",
      "command": "g++",
      "args": [
        "-std=c++17","-Wall","-g",
        "${workspaceFolder}/ET580-Project-1/Driver_Phase1.cpp",
        "${workspaceFolder}/ET580-Project-1/Container.cpp",
        "${workspaceFolder}/ET580-Project-1/Files.cpp",
        "-o","${workspaceFolder}/ET580-Project-1/dynamic_array.exe"
      ],
      "options": { "cwd": "${workspaceFolder}/ET580-Project-1", "shell": { "executable": "cmd.exe", "args": ["/d","/c"] } },
      "group": { "kind": "build", "isDefault": true },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Run (MinGW64)",
      "type": "shell",
      "command": "${workspaceFolder}\\ET580-Project-1\\dynamic_array.exe",
      "options": { "cwd": "${workspaceFolder}/ET580-Project-1", "shell": { "executable": "cmd.exe", "args": ["/d","/c"] } },
      "dependsOn": "Build (MinGW64)"
    }
    ,
    {
      "label": "Build (Johnson)",
      "type": "shell",
      "command": "g++",
      "args": [
        "-std=c++17","-Wall","-g",
        "${workspaceFolder}/ET580-Project-1/Driver_Johnson.cpp",
        "${workspaceFolder}/ET580-Project-1/Container.cpp",
        "${workspaceFolder}/ET580-Project-1/Files.cpp",
        "-o","${workspaceFolder}/ET580-Project-1/dynamic_array_johnson.exe"
      ],
      "options": { "cwd": "${workspaceFolder}/ET580-Project-1", "shell": { "executable": "cmd.exe", "args": ["/d","/c"] } },
      "group": { "kind": "build", "isDefault": false },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Run (Johnson)",
      "type": "shell",
      "command": "${workspaceFolder}\\ET580-Project-1\\dynamic_array_johnson.exe",
      "options": { "cwd": "${workspaceFolder}/ET580-Project-1", "shell": { "executable": "cmd.exe", "args": ["/d","/c"] } },
      "dependsOn": "Build (Johnson)"
    }
    ,
    {
      "label": "Build (Phase II)",
      "type": "shell",
      "command": "g++",
      "args": [
        "-std=c++17","-Wall","-g",
        "${workspaceFolder}/ET580-Project-1/Driver_Johnson.cpp",
        "${workspaceFolder}/ET580-Project-1/Container.cpp",
        "${workspaceFolder}/ET580-Project-1/Files.cpp",
        "-o","${workspaceFolder}/ET580-Project-1/dynamic_array_phase2.exe"
      ],
      "options": { "cwd": "${workspaceFolder}/ET580-Project-1", "shell": { "executable": "cmd.exe", "args": ["/d","/c"] } },
      "group": { "kind": "build", "isDefault": true },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Run (Phase II)",
      "type": "shell",
      "command": "${workspaceFolder}\\ET580-Project-1\\dynamic_array_phase2.exe",
      "options": { "cwd": "${workspaceFolder}/ET580-Project-1", "shell": { "executable": "cmd.exe", "args": ["/d","/c"] } },
      "dependsOn": "Build (Phase II)"
    }
  ]
}